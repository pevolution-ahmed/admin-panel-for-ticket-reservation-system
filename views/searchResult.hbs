{{#if authenticated }}
<div class="right_col">
      <h1 >{{title}}</h1>
 <br>
    {{#if userData}}
    <div class="panel panel-default" style="width :50%; color:black">
      <div class="panel-body">
        <ul>
          <li ><h3> <span class="glyphicon glyphicon-user"></span> Username : {{userData.fullName}}</h3><br></li>
          <li><h3> <span class="glyphicon glyphicon-credit-card"></span>  SSN : {{userData.nationalId}}</h3></li>
          <br>

          <h3>Tickets:</h3>
          
          <br>
          {{#if allData}}
          {{#each allData}}
          <div class="panel panel-default">

          <li class="panel-body"><h4> Class: {{this.className}}</h4></li>
          <li class="panel-body" ><h4> Block: {{this.blockNumber}}</h4></li>
          <li class="panel-body" ><h4> Stadium: {{this.stadiumName}}</h4></li>
          <li class="panel-body" ><h4> Seat: {{this.seatNumber}}</h4></li>
          <li class="panel-body" ><h4> Gate: {{this.NumberOfGates}}</h4></li>
          <li class="panel-body" ><h4> Match: {{this.HomeTeamName}} Vs {{this.AwayTeamName}}</h4></li>
          <li class="panel-body" ><h4> Start Time:  {{this.StartTime}}</h4></li>
          </div>
          {{/each}}
          {{else}}
            <h4 class="btn-info">User hasn't Reserve Any Tickets </h4>
          {{/if}}
        </ul>
        
      </div>
  </div>
          
        {{/if}} 
        
</div>
{{else}}
  
        <div class="alert alert-danger">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
    <h1 >Unauthenticated user</h1>
    <a href="/login"><h3 class="white">Back to login page</h3></a> 
  </div>  
{{/if}}